CREATE TABLE MENU (	
	MENUCD VARCHAR(50) NOT NULL, 
	PMENUCD VARCHAR(50), 
	depth varchar(10),
	menunm varchar(100),
	URLPATH VARCHAR(2000), 
	PARAM VARCHAR(2000), 
	USRTYPE VARCHAR(100), 
	SORTSEQ int, 
	USEFLAG VARCHAR(1) DEFAULT 'Y', 
	STS VARCHAR(1) DEFAULT 'C' /* 상태 */, 
	REGCD VARCHAR(50), 
	REGDT DATE, 
	MODCD VARCHAR(50), 
	MODDT DATE
) 